{
    "description": "Fixes video brightness issues on youtube. See https://github.com/LukasWillin/youtube-video-brightness-fixer",
    "manifest_version": 2,
    "name": "Enlight-YT",
    "version": "0.1",
    "homepage_url": "https://github.com/LukasWillin/youtube-video-brightness-fixer",
    "icons": {
        "48": "icons/yt-brf.jpg"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "brightness-fix.js"
            ],
            "css": [
                "brightness-fix.css"
            ]
        }
    ]
}